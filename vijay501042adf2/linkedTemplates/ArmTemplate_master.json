{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vijay501042adf2"
		},
		"ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2'"
		},
		"AzureSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQLDB'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vijay501042sa2.dfs.core.windows.net/"
		},
		"AzureSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "vijay501042server1.database.windows.net"
		},
		"AzureSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "vijay501042_db1"
		},
		"AzureSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vijay501042"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vijay501042adf2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_accountKey": {
						"value": "[parameters('ADLSGen2_accountKey')]"
					},
					"AzureSQLDB_password": {
						"value": "[parameters('AzureSQLDB_password')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"AzureSQLDB_properties_typeProperties_server": {
						"value": "[parameters('AzureSQLDB_properties_typeProperties_server')]"
					},
					"AzureSQLDB_properties_typeProperties_database": {
						"value": "[parameters('AzureSQLDB_properties_typeProperties_database')]"
					},
					"AzureSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}